alias rfid="cd /home/pi/rfid/"
alias temp='vcgencmd measure_temp'
alias 0="cd /home/pi/rfid/ ; sh menu.sh"
alias 1="cd /home/pi/rfid/ ; sh status-service.sh"
alias 2="cd /home/pi/rfid/ ; sh restart-service.sh"
alias 3="cd /home/pi/rfid/ ; sh start-service.sh"
alias 4="cd /home/pi/rfid/ ; sh stop-service.sh"
alias 5="cd /home/pi/rfid/ ; sh add-user.sh"
alias 6="cd /home/pi/rfid/ ; tail --lines=20 door-access.log"
